  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run GBMain-2",
      "request": "launch",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npx",
      "args": [
        "cds",
        "watch",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env2",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/GBMain\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}